		<init-param>
            <name>view-jsp</name>
			<value>/html/jsp/portlet/users/view/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
        <supported-locale>uk_UA</supported-locale>
        <supported-locale>ru_RU</supported-locale>
        <resource-bundle>i18n.Resource</resource-bundle>
		<portlet-info>
			<title>FidoArp Users</title>
			<short-title>FidoArp Users</short-title>
			<keywords>users</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
