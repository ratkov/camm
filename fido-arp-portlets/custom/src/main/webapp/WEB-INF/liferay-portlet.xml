        <instanceable>false</instanceable>
        <header-portlet-css>/html/css/bootstrap.css</header-portlet-css>
        <header-portlet-css>/html/css/main.css</header-portlet-css>
        <footer-portlet-javascript>/html/js/jquery-1.9.1.min.js</footer-portlet-javascript>
        <footer-portlet-javascript>/html/js/jquery.serialize-object.js</footer-portlet-javascript>
        <footer-portlet-javascript>/html/js/jquery.validate.js</footer-portlet-javascript>
        <footer-portlet-javascript>/html/js/partner-product-type/partner-product-type.js</footer-portlet-javascript>
        <css-class-wrapper>fido-arp-partner-product-type</css-class-wrapper>
    </portlet>
    <portlet>
        <portlet-name>fido_arp_users</portlet-name>
        <instanceable>false</instanceable>
        <header-portlet-css>/html/css/main.css</header-portlet-css>
