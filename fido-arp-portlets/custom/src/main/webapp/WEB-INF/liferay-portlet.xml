		<portlet-name>fido_arp_partners</portlet-name>
		<instanceable>false</instanceable>
		<header-portlet-css>/html/css/main.css</header-portlet-css>
		<footer-portlet-javascript>/html/js/main.js</footer-portlet-javascript>
		<css-class-wrapper>fido-arp-partners-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>fido_arp_queues</portlet-name>
		<instanceable>false</instanceable>
        <header-portlet-css>/html/css/bootstrap.css</header-portlet-css>
        <header-portlet-css>/html/css/ui/jquery-ui-1.10.3.theme.css</header-portlet-css>
