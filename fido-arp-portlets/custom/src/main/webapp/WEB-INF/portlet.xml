<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
    <portlet>
        <portlet-name>fido_arp_partners</portlet-name>
        <display-name>FidoARP Partners</display-name>
        <portlet-class>com.fidoarp.portlet.PartnerPortlet</portlet-class>
        <init-param>
            <name>view-jsp</name>
            <value>/html/jsp/portlet/partners/view/view.jsp</value>
        </init-param>
        <expiration-cache>0</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
        </supports>
        <supported-locale>uk_UA</supported-locale>
        <supported-locale>ru_RU</supported-locale>
        <resource-bundle>i18n.Resource</resource-bundle>
        <portlet-info>
            <title>FidoArp Partners</title>
            <short-title>FidoArp Partners</short-title>
            <keywords>partners</keywords>
        </portlet-info>
        <security-role-ref>
            <role-name>administrator</role-name>
        </security-role-ref>
        <security-role-ref>
            <role-name>power-user</role-name>
        </security-role-ref>
        <security-role-ref>
            <role-name>user</role-name>
        </security-role-ref>
    </portlet>
    <portlet>
        <portlet-name>fido_app_status</portlet-name>
        <display-name>FidoARP App Status</display-name>
        <portlet-class>com.fidoarp.portlet.AppStatusPortlet</portlet-class>
        <init-param>
            <name>view-jsp</name>
            <value>/html/jsp/portlet/app-status/view/view.jsp</value>
        </init-param>
        <expiration-cache>0</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
        </supports>
        <supported-locale>uk_UA</supported-locale>
        <supported-locale>ru_RU</supported-locale>
        <resource-bundle>i18n.Resource</resource-bundle>
        <portlet-info>
            <title>FidoArp App Status</title>
            <short-title>FidoArp App Status</short-title>
            <keywords>app status</keywords>
        </portlet-info>
        <security-role-ref>
            <role-name>administrator</role-name>
        </security-role-ref>
        <security-role-ref>
            <role-name>power-user</role-name>
        </security-role-ref>
        <security-role-ref>
            <role-name>user</role-name>
        </security-role-ref>
    </portlet>
    <portlet>
        <portlet-name>fido_arp_partner_product_type</portlet-name>
        <display-name>FidoARP Partners Product Type</display-name>
        <portlet-class>com.fidoarp.portlet.PartnerProductTypePortlet</portlet-class>
        <init-param>
            <name>view-jsp</name>
            <value>/html/jsp/portlet/partner-product-type/view/view.jsp</value>
        </init-param>
        <expiration-cache>0</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
        </supports>
        <supported-locale>uk_UA</supported-locale>
        <supported-locale>ru_RU</supported-locale>
        <resource-bundle>i18n.Resource</resource-bundle>
        <portlet-info>
            <title>FidoArp Partners Product Type</title>
            <short-title>FidoArp Partners Product Type</short-title>
            <keywords>partners,product,type,template</keywords>
        </portlet-info>
        <security-role-ref>
            <role-name>administrator</role-name>
        </security-role-ref>
        <security-role-ref>
            <role-name>power-user</role-name>
        </security-role-ref>
        <security-role-ref>
            <role-name>user</role-name>
        </security-role-ref>
    </portlet>
	<portlet>
		<portlet-name>fido_arp_queues</portlet-name>
		<display-name>FidoARP Queues</display-name>
		<portlet-class>com.fidoarp.portlet.QueuePortlet</portlet-class>
        <init-param>
            <name>edit-jsp</name>
            <value>/html/jsp/portlet/queues/edit/edit.jsp</value>
        </init-param>
		<init-param>
            <name>view-jsp</name>
			<value>/html/jsp/portlet/queues/view/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>VIEW</portlet-mode>
        </supports>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>EDIT</portlet-mode>
            <window-state>NORMAL</window-state>
        </supports>
        <supported-locale>uk_UA</supported-locale>
        <supported-locale>ru_RU</supported-locale>
        <resource-bundle>i18n.Resource</resource-bundle>
		<portlet-info>
			<title>FidoArp Queues</title>
			<short-title>FidoArp Queues</short-title>
			<keywords>queues</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
    <portlet>
        <portlet-name>fido_arp_users</portlet-name>
        <display-name>FidoARP Users</display-name>
        <portlet-class>com.fidoarp.portlet.UserPortlet</portlet-class>
        <init-param>
            <name>edit-jsp</name>
            <value>/html/jsp/portlet/users/edit/edit.jsp</value>
        </init-param>
        <init-param>
            <name>view-jsp</name>
            <value>/html/jsp/portlet/users/view/view.jsp</value>
        </init-param>
        <expiration-cache>0</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>VIEW</portlet-mode>
        </supports>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>EDIT</portlet-mode>
            <window-state>NORMAL</window-state>
        </supports>
        <supported-locale>uk_UA</supported-locale>
        <supported-locale>ru_RU</supported-locale>
        <resource-bundle>i18n.user.Resource</resource-bundle>
        <portlet-info>
            <title>FidoArp Users</title>
            <short-title>FidoArp Users</short-title>
            <keywords>users</keywords>
        </portlet-info>
        <security-role-ref>
            <role-name>administrator</role-name>
        </security-role-ref>
        <security-role-ref>
            <role-name>power-user</role-name>
        </security-role-ref>
        <!--<security-role-ref>-->
            <!--<role-name>user</role-name>-->
        <!--</security-role-ref>-->
    </portlet>


</portlet-app>